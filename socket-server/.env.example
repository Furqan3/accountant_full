# ==============================================
# SOCKET SERVER - ENVIRONMENT CONFIGURATION
# ==============================================
# Copy this file to .env and fill in your values

# ==============================================
# SERVER CONFIGURATION
# ==============================================

PORT=4000
NODE_ENV=development

# ==============================================
# CORS - ALLOWED ORIGINS
# ==============================================
# Comma-separated list of allowed origins for WebSocket connections

ALLOWED_ORIGINS=http://localhost:3000,http://localhost:3001

# ==============================================
# SUPABASE CONFIGURATION
# ==============================================
# Get these from: https://supabase.com/dashboard/project/_/settings/api
# Service role key required for server-side operations

SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# ==============================================
# APP URLs
# ==============================================
# Used for email links and redirects

FRONTEND_URL=http://localhost:3000
ADMIN_URL=http://localhost:3001

# ==============================================
# EMAIL CONFIGURATION (Resend)
# ==============================================
# Required for sending notifications via socket server
# Get your API key from: https://resend.com/api-keys
# Add your domain at: https://resend.com/domains

RESEND_API_KEY=re_UcqQ4Tsc_KjVNryKeu86gQWMM9LQk4icA
EMAIL_FROM=FilingHub 
